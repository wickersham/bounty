<!-- This is the display form for each program displayed on publicView from storage (mongo) -->
<p>
  {{#each programs}}
  <div class="list-group">
    <div class="list-group-item">
      <h4 class="list-group-item-heading"><strong>{{this.programName}}</strong></h4>
      <div class="list-group-item-text">
        {{this.description}}<br>
      </div>

      <div class="list-group-item-text more-info" style = "display:none">
        Phone Number: {{this.phoneNumber}}<br>
        Location: {{this.location}}<br>
        County: {{this.county}}<br>
        Email: {{this.email}}<br>
        Cost: {{this.cost}}<br>
        Meals Provided: {{this.meals}}<br>
        Minimum Age: {{this.ageMin}}<br>
        Maximum Age: {{this.ageMax}}<br>
        Program Url: {{this.programUrl}}<br>
        Affiliations: {{this.affiliation}}<br>
        Other Benefits: {{this.otherBenefits}}<br>
        Provided Certification: {{this.certificate}}<br>
        Timeline: {{this.timeline}}<br>
        Time of Day:{{this.timeOfDay}}<br>
        Past Program Successes: {{this.pastSuccess}}<br>
        Past Participants: {{this.pastParticipants}}<br>
        Word Out: {{this.wordOut}}<br>
        How Funded: {{this.funded}}<br>
        Current Impediments: {{this.impediments}}<br>
        Average Number of Participants: {{this.avgNumParticip}}<br>
        Contact Name: {{this.contactName}}<br>
        Partners: {{this.partners}}<br>
        Scholarships: {{this.scholarships}}<br>
        Parental Involvment: {{this.parentsInvolved}}<br>
        How Success is Measured: {{this.measureSuccess}}<br>
        NCFL Funded: {{this.NCFLFunded}}<br>
        Bring Day One: {{this.bringDayOne}}<br>
        In person: {{this.inPerson}}
      </div>
      <button type="button" class = "btn btn-success">More info</button>

    </div>
  </div>
  {{/each}}
</p>
